{
    "exclude": [
        "node_modules",
        "build",
        "./typings"
    ],
    "files": [
        "src/server.ts"
        
    ],
    "compileOnSave": false,
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "declaration": false,
        "outDir": "build"
    }
   
}